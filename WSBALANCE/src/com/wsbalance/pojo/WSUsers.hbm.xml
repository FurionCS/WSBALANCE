<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wsbalance.pojo">
	 <class name="WSUsers" table="T_WSBALANCE_USERS"> 
	 	<id name="WSUID" column="WSUID">
	 		<generator class="native" />
	 	</id>
	 	<property name="WSUWXNUM" column="WSUWXNUM" type="string" length="100"></property>
	 	<property name="WSUNAME" column="WSUNAME" type="string" length="10"></property>
	 	<property name="WSUTEL" column="WSUTEL" type="string" length="20"></property>
	 	<property name="WSUIDCARD" column="WSUIDCARD" type="string" length="18"></property>
	 	<property name="WSPUID" column="WSPUID" type="integer"></property>
	 	<property name="WSLEVEL" column="WSLEVEL" type="integer"></property>
	 	<property name="WSAUTHORIZATION" column="WSAUTHORIZATION" type="string" length="50"></property>
	 	<property name="WSCREATEDAY" column="WSCREATEDAY" type="timestamp"></property>
	 </class> 

</hibernate-mapping>